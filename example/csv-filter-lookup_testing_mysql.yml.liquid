exec:
  max_threads: 1
  min_output_tasks: 1
in:
  type: mysql
  host: localhost
  user: root
  password: 'passsword'
  database: test
  table: customer
filters:
  - type: csv_lookup
    mapping_from:
      - car_name
      - company
    mapping_to:
      - model
      - brand
    new_columns:
      - { name: car_id, type: long }
      - { name: category, type: string }
      - { name: fuel_capacity, type: string }
    path_of_lookup_file: "..path../car.csv"
out:
  type: mysql
  host: localhost
  user: root
  password: 'passsword'
  database: test
  table: output_table
  mode: truncate_insert