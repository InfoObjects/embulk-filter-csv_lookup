exec:
  max_threads: 1
  min_output_tasks: 1
in:
  type: postgresql
  host: localhost
  port: 5432
  user: postgres
  password: 1234
  schema: public
  database: test
  table: database_table
filters:
  - type: csv_lookup
    mapping_from:
    - id
    - name
    mapping_to:
     - ID
     - Name
    new_columns:
     - { name: Pin, type: string }
     - { name: Gender, type: string }
     - { name: Phone_Number, type: string }
    path_of_lookup_file: ../lookup_table.csv
out:
  type: postgresql
  host: localhost
  port: 5432
  database: test
  user: postgres
  password: 1234
  schema: public
  table:  output_table_1
  mode: truncate_insert
